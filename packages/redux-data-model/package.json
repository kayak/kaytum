{
  "name": "redux-data-model",
  "version": "0.11.0",
  "description": "Opinionated, Redux abstraction with built-in immutability, normalization, and selectors support.",
  "author": "Lucas Lira Gomes <llira@kayak.com>",
  "homepage": "https://kayak.github.io/redux-data-model/",
  "license": "Apache-2.0",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kayak/redux-data-model.git"
  },
  "scripts": {
    "clean": "rimraf dist js_coverage .rpt2_cache",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --silent",
    "test:logs": "jest",
    "test:update": "npm run test -- -u",
    "test:watch": "npm run test -- --watchAll --verbose",
    "test:coverage": "npm run test -- --verbose --coverage",
    "coveralls": "coveralls",
    "build": "npm run lint && npm run test && rollup -c",
    "size": "npm run build && size-limit"
  },
  "bugs": {
    "url": "https://github.com/kayak/redux-data-model/issues"
  },
  "peerDependencies": {
    "immer": "^6.0.1",
    "lodash": "^4.17.15",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": ">=4.4.9",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "jest": "25.1.0",
    "jest-extended": "0.11.2",
    "jsdom-global": "3.0.2",
    "react-test-renderer": "16.10.2",
    "redux-mock-store": "1.5.3",
    "ts-jest": "25.5.1"
  }
}
