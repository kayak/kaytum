  "bugs": {
    "url": "https://github.com/kayak/react-resux/issues"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-resux": "^0.6.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.2",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "jsdom-global": "^3.0.2",
    "react-resux": "^0.6.2",
    "react-test-renderer": "^16.10.2",
